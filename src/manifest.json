{
  "name": "ChatGPT Soliloquy",
  "description": "ChatGPT-Soliloquy is a Chrome extension that allows ChatGPT to engage in a conversation with itself.",
  "version": "0.0.3",
  "manifest_version": 3,
  "icons": {
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/chat"],
      "js": ["chatgpt-soliloquy.js"]
    }
  ]
}
